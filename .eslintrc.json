{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "localStorage": true,
    "document": true,
    "window": true,
    "fetch": false
  },
  "rules": {
    "no-unused-vars": "warn",
    "class-methods-use-this": "off",
    "react/no-array-index-key": "off",
    "react/prop-types": "warn",
    "jsx-a11y/click-events-have-key-events": false,
    "jsx-a11y/no-static-element-interactions": false,
    "jsx-a11y/aria-role": [
      2,
      {
        "ignoreNonDOM": true
      }
    ]
  }
}
